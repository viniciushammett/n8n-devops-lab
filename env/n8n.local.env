# --- Public URL / Server ---
N8N_HOST=localhost
N8N_PROTOCOL=http
N8N_PORT=5678
# If exposing behind a reverse proxy with TLS, set WEBHOOK_URL to your https URL
WEBHOOK_URL=http://localhost:5678/

# --- Database ---
POSTGRES_USER=n8n
POSTGRES_PASSWORD=n8n
POSTGRES_DB=n8n

# --- Security ---
# Generate a long random key in production
N8N_ENCRYPTION_KEY=change-this-to-a-long-random-string
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=change-me